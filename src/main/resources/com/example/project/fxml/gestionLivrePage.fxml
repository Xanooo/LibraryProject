<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="353.0" prefWidth="216.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.example.project.controllers.GestionLivreController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="Gestion des utilisateurs" >
        <font><Font size="15.0"/> </font>
    </Label>
    <Button onAction="#onAddButtonClick" text="Ajouter un livre" />
    <Button onAction="#onEditButtonClick" text="Modifier un livre" />
    <Button text="Supprimer un livre" />
    <Button onAction="#onRetourButtonClick" text="Retour à l'accueil" />
    <Label fx:id="credits" text="Projet développé par Xano, 2024." underline="true">
        <font>
            <Font size="9.0" />
        </font>
    </Label>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
